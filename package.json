{
    "name": "nextportfolio",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "export": "next export",
        "deploy": "npm run build && npm run export",
        "start": "next start",
        "lint": "next lint",
        "start:production": "NODE_ENV=production node server.js",
        "prismaGenerate": "dotenv -e .env.local -- npx prisma generate",
        "prismaPush": "dotenv -e .env.local -- npx prisma db push",
        "prismaSeed": "dotenv -e .env.local -- npx prisma db seed",
        "prismaDev": "dotenv -e .env.local -- npx prisma migrate dev"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts",
        "schema": "src/prisma/schema.prisma"
    },
    "dependencies": {
        "@apollo/client": "^3.5.10",
        "@emotion/cache": "^11.7.1",
        "@emotion/react": "^11.8.2",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.8.1",
        "@fontsource/roboto": "^4.5.5",
        "@mui/icons-material": "^5.4.4",
        "@mui/material": "^5.5.2",
        "@mui/styled-engine-sc": "^5.5.2",
        "@prisma/client": "^3.11.1",
        "add": "^2.0.6",
        "apollo-server-micro": "^3.6.3",
        "dotenv-cli": "^5.0.0",
        "emotion": "^11.0.0",
        "framer-motion": "^6.2.8",
        "graphql": "^16.3.0",
        "micro": "^9.3.4",
        "micro-cors": "^0.1.1",
        "moment": "^2.29.1",
        "next": "^12.1.0",
        "next-themes": "^0.0.15",
        "nexus": "^1.3.0",
        "nexus-plugin-prisma": "^0.35.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "styled-components": "^5.3.5",
        "yarn": "^1.22.17"
    },
    "devDependencies": {
        "@types/micro-cors": "^0.1.2",
        "@types/node": "17.0.0",
        "@types/react": "17.0.37",
        "@types/styled-components": "^5.1.20",
        "eslint": "8.4.1",
        "eslint-config-next": "12.0.7",
        "prisma": "^3.11.1",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "4.5.4"
    }
}